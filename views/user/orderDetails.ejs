                        method: "PUT",
                        headers: {
                            "Content-Type": "application/json"
                        },
                        body: JSON.stringify(data)
                    });

                    if (response.ok) {
                        const responseData = await response.json();
                        if (responseData.message) {
                            document.getElementById("status").innerText = responseData.status;
                            document.getElementById("submit").style.display = "none";
                            document.getElementById('wallet').hidden = false;
                        } else {
                            document.getElementById("status").innerText = responseData.status;
                            document.getElementById("submit").style.display = "none";
                        }
                    } else {
                        throw new Error('Failed to cancel order');
                    }
                }
            } catch (error) {
                console.error('Error cancelling order:', error);
                Swal.fire({
                    icon: 'error',
                    title: 'Oops...',
                    text: 'Something went wrong while cancelling the order. Please try again later.',
                });
            }
        });

        const returnBtn = document.getElementById("returns").addEventListener("click", async function () {
            try {
                const confirmed = await Swal.fire({
                    title: 'Are you sure you want to cancel this item?',
                    icon: 'question',
                    showCancelButton: true,
                    confirmButtonText: 'Yes',
                    cancelButtonText: 'No'
                });

                if (confirmed.isConfirmed) {
                    const { value: reason } = await Swal.fire({
                        title: 'Select a reason for cancellation',
                        input: 'select',
                        inputOptions: {
                            'reason1': 'Shoe has not an expected size',
                            'reason2': 'Shoe is damaged',
                            'reason3': 'Quality is not good'
                        },
                        inputPlaceholder: 'Select a reason',
                        showCancelButton: true,
                        confirmButtonText: 'Confirm',
                        cancelButtonText: 'Cancel',
                        inputValidator: (value) => {
                            if (!value) {
                                return 'You need to select a reason';
                            }
                        }
                    });

                    if (reason) {
                        const status = document.getElementById("status").textContent;
                        const orderId = document.getElementById("status").getAttribute("data-id");
                        const data = { orderId, status, reason };

                        const response = await fetch('/return-order', {
                            method: "PUT",
                            headers: {
                                "Content-Type": "application/json"
                            },
                            body: JSON.stringify(data)
                        });

                        if (response.ok) {
                            const responseData = await response.json();
                            console.log(responseData);
                            document.getElementById("status").innerText = responseData.status;
                            document.getElementById("returns").style.display = "none";
                            document.getElementById('wallet').style.display = 'block';
                            document.getElementById('return').innerHTML = responseData.message;
                        }
                    }
                }
            } catch (error) {
                console.log(error);
                Swal.fire({
                    icon: 'error',
                    title: 'Oops...',
                    text: 'Something went wrong while cancelling the order. Please try again later.',
                });
            }
        });

        // Get the HTML element
        const returnDateElement = document.getElementById('returnDate');

        // Get the current date
        const currentDate = moment();

        // Calculate the return expiry date (7 days from now)
        const returnExpiryDate = currentDate.add(7, 'days');

        // Update the HTML element with the calculated expiry date
        returnDateElement.innerText = returnExpiryDate.format('YYYY-MM-DD');

        // Set up a timer to check expiry date and hide the element if exceeded
        const timer = setInterval(() => {
            const now = moment();
            if (now.isAfter(returnExpiryDate)) {
                returnDateElement.style.display = 'none'; // Hide the element
                document.getElementById('returns').style.display = "none"
                clearInterval(timer); // Stop the timer
            }
        }, 1000); // Check every second

    </script>
</body>


