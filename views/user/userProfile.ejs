        // Function to validate ZIP code format
        function isValidZip(zip) {
            // Check if the ZIP code is exactly 6 digits and not all zeros
            return /^\d{6}$/.test(zip) && !/^0{6}$/.test(zip);
        }


        // Function to validate mobile number format
        function isValidMobile(mobile) {
            // Check if the mobile number is exactly 10 digits and not all zeros
            return /^(?!0+$)\d{10}$/.test(mobile);
        }

