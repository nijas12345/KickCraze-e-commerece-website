            console.log(confirmPassword);
            console.log(userId);

            const oldPasswordError = document.getElementById('oldpass-error')
            const newPasswordError = document.getElementById('newpass-error')
            const confirmPasswordError = document.getElementById('newcpass-error')

            oldPasswordError.textContent = ""
            newPasswordError.textContent = ""
            confirmPasswordError.textContent = ""


            let isValid = true


            if (oldPassword === '') {
                oldPasswordError.textContent = 'Password is required';
                isValid = false;
                return;
            } else if (oldPassword.length < 8) {
                oldPasswordError.textContent = 'Password must be at least 8 characters long';
                isValid = false;
                return;
            }


            if (password === '') {
                newPasswordError.textContent = 'Password is required';
                isValid = false;
                return;
            } else if (password.length < 8) {
                newPasswordError.textContent = 'Password must be at least 8 characters long';
                isValid = false;
                return;
            }
            else if (!/[A-Z]/.test(password) || !/[a-z]/.test(password)) {
