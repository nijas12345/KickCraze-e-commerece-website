                    <div style="display: flex;"><span>Name:   <%=orders.addressId.name %></span></div>
                    <div style="display: flex;"><span>houseName:   <%=orders.addressId.houseName %></span></div>
                    <div style="display: flex;"><span>state:   <%=orders.addressId.state %></span></div>
                    <div style="display: flex;"><span>Phone:   <%=orders.addressId.mobile %></span></div>
                    <div style="display: flex;"><span>Pincode:   <%=orders.addressId.pin %></span></div>
                </div>
            </div>
         
            <!-- Download button -->
            <div class="download-button">
                <button onclick="downloadInvoice()">Download PDF</button>
            </div>
        </div>
