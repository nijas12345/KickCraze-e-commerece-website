            <h1 class="headline">Kickzone<span style="color: #000;">.</span><span style="color: #6495ED;">online</span></h1>
            <p class="tagline">The Ultimate Destination for Premium Shoes</p>
            <br>
            <h2>Invoice</h2>
            <!-- Invoice details -->
            <div class="invoice-details">
                <ul>
                    <div class="invoice-number">
                        <input type="hidden" value="<%=orders._id %>" name="" id="orderId">
                        <label for="invoice-number">Invoice Number:</label>
                        <input type="text" id="invoice-number" readonly>
                    </div>
                    <li>Invoice Date: <%= orders.orderedDate.toLocaleDateString('en-US', { year: 'numeric', month: 'numeric', day: 'numeric' }) %></li>
                    <li>Customer Name: <%= orders.addressId.name %></li>
                    <!-- Add more invoice details as needed -->
                </ul>
             
            </div>
            <!-- Invoice table -->
            <div class="invoice-table">
                <table class="table">
                    <thead>
                        <tr>
                            <th>Product</th>
                            <th>Quantity</th>
                            <th>total</th>
                            <th>Discounted Price</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% orders.products.forEach((product) => { %>
                        <tr>
                            <td><%= product.productId.name %></td>
                            <td><%= product.quantity %></td>
                            <td><%= product.totalPrice %></td>
                            <% if (orders.discountTotal) { %>
                            <td><%= orders.discountTotal %></td>
                            <% } else { %>
                            <td><%= product.total %></td>
                            <% } %>
                        </tr>
                        <% }) %>
                    </tbody>
                </table>
            </div>
            <!-- Invoice total -->
            <p style="margin-left: 500px;">Total: <% if (orders.discountTotal) { %>₹ <%= orders.discountTotal %> <% } else { %>₹ <%= total %> <% } %></p>
