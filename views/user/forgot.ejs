<body class="theme-color2 light ltr">


    <!-- Sign Up Section Start -->
    <div class="login-section">
        <div class="materialContainer">
            <div class="box">
                <div class="login-title">
                    <h2>Forgot Password</h2>
                </div>
                <% if(typeof message !=="undefined" ){ %>
                    <p style="color:red; margin-left: -135px ;">
                        <%=message %>
                    </p>
                    <% } %>
                        <form action="/forgot-email" method="post">
                            <div class="input">
                                <label for="emailname">Enter Email Address</label>
                                <input type="text" name="name" class="is-invalid" id="emailname">
                                <span class="spin"></span>
                            </div>


                            <div class="button login button-1">
                                <button onclick="location.href ='/forgot-email'">
                                    <span>Send</span>
                                    <i class="fa fa-check"></i>
                                </button>
                            </div>
                        </form>
            </div>
        </div>
    </div>
    <!-- Sign Up Section End -->

