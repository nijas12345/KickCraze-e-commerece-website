<body class="signup-page theme-color2">

    <!-- Sign Up Section Start -->


    <form action="/confirm-password" id="form" method="post">
        <div class="login-section">
            <div class="materialContainer">
                <div class="box">
                    <div class="login-title">
                        <h2>Change Password</h2>
                    </div>
                    <% if(typeof message !=="undefined" ){ %>
                        <p style="color:red; margin-left: -50px ;">
                            <%=message %>
                        </p>
                        <% } %>
                            <div style="display: flex; margin-left: 10px;" id="pass-error"></div>
                            <div class="input">

                                <input type="password" name="pass" id="pass" placeholder="New Password" required>

                            </div>
                            <div style="display: flex; margin-left: -5px ; color: red;" id="compass-error"></div>
                            <div class="input">


                                <input type="password" name="cpass" id="compass" placeholder="Confirm Password"
                                    required>

                            </div>
                            <br><br>
                            <div class="button login">
                                <button type="button" id="submitBtn">
                                    <span>Save Changes</span>
                                    <i class="fa fa-check"></i>
                                </button>
                            </div>

                            <div class="button login">
                                <button onclick="window.location.href='/login'"
                                    style="width: 100px; margin-left: 300px;">
                                    <span>Log In</span>
                                    <i class="fa fa-check"></i>
                                </button>
                            </div>
                </div>
            </div>
        </div>
    </form>








