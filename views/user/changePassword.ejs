    <script>
        document.getElementById('submitBtn').addEventListener('click', function () {

            const password = document.getElementById('pass').value;
            const confirmPassword = document.getElementById('compass').value;
            console.log("password", password);
            const passError = document.getElementById('pass-error');
            const compassError = document.getElementById('compass-error');


            // Reset error messages

            passError.textContent = '';
            compassError.textContent = '';

            let isValid = true;

            // Name validation


            // Password validation
            if (password === '') {
                passError.textContent = 'Password is required';
                isValid = false;
            } else if (password.length < 8) {
                passError.textContent = 'Password must be at least 8 characters long';
                isValid = false;
            }
            else if (!/[A-Z]/.test(password) || !/[a-z]/.test(password)) {
                passError.textContent = 'Confirm password must contain at least one uppercase and one lowercase letter';
                isValid = false;
            }

            // Confirm Password validation
            if (confirmPassword === '') {
                compassError.textContent = 'Confirm password is required';
                isValid = false;
            } else if (password !== confirmPassword) {
                compassError.textContent = 'Passwords do not match';
                isValid = false;
            }

            // If form is valid, proceed with submission
            if (isValid) {
                document.getElementById('form').submit();
            }
        });


    </script>
