    <script>
        document.getElementById('submitBtn').addEventListener('click', function () {
            const name = document.getElementById('name').value.trim();
            const email = document.getElementById('email').value.trim();
            const mobile = document.getElementById('mobileno').value.trim();
            const password = document.getElementById('pass').value;
            const confirmPassword = document.getElementById('compass').value;
            const refferal = document.getElementById('refer').value.trim()

            const nameError = document.getElementById('name-error');
            const emailError = document.getElementById('email-error');
            const phoneError = document.getElementById('phone-error');
            const passError = document.getElementById('pass-error');
            const compassError = document.getElementById('compass-error');


            // Reset error messages
            nameError.textContent = '';
            emailError.textContent = '';
            phoneError.textContent = '';
            passError.textContent = '';
            compassError.textContent = '';

            let isValid = true;

            // Name validation
            if (name === '') {
                nameError.textContent = 'Name is required';
                isValid = false;
            }
            else if (/\d|-/.test(name)) {
                nameError.textContent = 'Name cannot contain digits or hyphens';
                isValid = false;
            }


            // Email validation
            if (email === '') {
                emailError.textContent = 'Email is required';
                isValid = false;
            } else if (!isValidEmail(email)) {
                emailError.textContent = 'Invalid email format';
                isValid = false;
            }


            if (mobile === '') {
                phoneError.textContent = 'Phone number is required';
                isValid = false;
            } else if (!isValidMobile(mobile)) {
                phoneError.textContent = 'Phone number must contain at least 10 digits and should not contain 10 zeros';
                isValid = false;
            }

            // Password validation
            if (password === '') {
                passError.textContent = 'Password is required';
                isValid = false;
            } else if (password.length < 8) {
                passError.textContent = 'Password must be at least 8 characters long';
                isValid = false;
            }
            else if (!/[A-Z]/.test(password) || !/[a-z]/.test(password)) {
                passError.textContent = 'Confirm password must contain at least one uppercase and one lowercase letter';
                isValid = false;
            }

            // Confirm Password validation
            if (confirmPassword === '') {
                compassError.textContent = 'Confirm password is required';
                isValid = false;
            } else if (password !== confirmPassword) {
                compassError.textContent = 'Passwords do not match';
                isValid = false;
            }

            // If form is valid, proceed with submission
            if (isValid) {
                document.getElementById('form').submit();
            }
        });

        // Function to validate email format
        function isValidEmail(email) {
            // Regular expression for email validation
            const emailRegex = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
            return emailRegex.test(email);
        }

        // Function to validate mobile number
        function isValidMobile(mobile) {
            // Regular expression to check if the mobile number contains only digits
            const regex = /^[0-9]+$/;

            // Check if the mobile number contains at least 10 digits and doesn't contain 10 zeros
            return regex.test(mobile) && mobile.length >= 10 && !/^0{10}$/.test(mobile);
        }


    </script>
