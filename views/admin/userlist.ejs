<script>
  document.querySelectorAll('.block-toggle').forEach(button=>{
    button.addEventListener('click',function(){
    const userId = this.dataset.id;
    fetch('/admin/user-block',{
    method:"PUT",
    headers:{
        'Content-Type':'application/json'
    },
    body:JSON.stringify({
        id:userId,
    })
  })
  .then((response)=>response.json())
  .then((data)=>{
    console.log(data.userStatus);
    
    if(data.userStatus){
          this.textContent = "BLOCK";
          this.classList.remove("btn-primary");
          this.classList.add("btn-danger");
    }else{
          this.textContent = "UNBLOCK";
          this.classList.remove("btn-danger");
          this.classList.add("btn-primary");
    }
    const badge = document.getElementById(`user-status-${userId}`);
      if (badge) {
        if (data.userStatus) {
          badge.textContent = "Unblocked";
          badge.classList.remove("alert-danger");
          badge.classList.add("alert-success");
        } else {
          badge.textContent = "Blocked";
          badge.classList.remove("alert-success");
          badge.classList.add("alert-danger");
        }
      }
  })
    })
  })
  
</script>


