                                <option>Pending</option>
                                <option>Shipped</option>
                                <option>Cancelled</option>
                                <option>Delivered</option>
                            </select>
                          
                        </div>
                    </div>
                </header> <!-- card-header end// -->
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th>#ID</th>
                                    <th scope="col">Name</th>
                                    <th scope="col">Email</th>
                                    <th scope="col"></th>
                                    <th scope="col">Status</th>
                                    <th scope="col">Date</th>
                                    <th scope="col">Edit Status</th>
                                    <th scope="col" class="text-end"> Action </th>
                                </tr>
                            </thead>
                            <tbody>

                               <%orders.forEach((order)=>{%>

                              
                                <tr>
                                    <%let orderId = order._id.toString().slice(8,13)%>

                                    <td><%= orderId %></td>
                                    <td><b><%=order.userId.name%></b></td>
                                    <td><template class="__cf_email__" data-cfemail="aec3cfdcd8c7c0eecbd6cfc3dec2cb80cdc1c3"><%=order.userId.name %></template></td>
                                    <td></td>
                                    <% if(order.status == "cancelled") {%>
                                    
                                    <td><span id="order-id" data-order.id="<%=order._id %>" class="badge rounded-pill alert-warning"><%= order.status %>  </span></td>
                                     <% }else{%>
                                        <td><span id="order-id" class="badge rounded-pill alert-success"><%= order.status %>  </span></td>
                                     <%} %>
                                    <td><%=order.createdAt.toLocaleDateString('en-US', { year: 'numeric', month: 'numeric', day: 'numeric' })%></td>
                                    <td>   <select id="my-select" data-id="<%=order._id %>" name="selection">
                                        <option value="pending" >Pending</option>
                                        <option value="shipped">Shipped</option>
                                        <!-- <option value="cancelled">Cancelled</option> -->
                                        <option value="delivered">Delivered</option>
                                    </select></td>
                                    <td class="text-end">
                                        <a href="/admin/cancel-orders?id=<%=order._id %>" class="btn btn-md rounded font-sm">Cancel</a>
